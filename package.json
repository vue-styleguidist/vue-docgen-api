{
  "name": "vue-docgen-api",
  "version": "0.0.0-development",
  "description": "Toolbox to extract information from Vue component files for documentation generation purposes.",
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-docgen-api/issues"
  },
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint . --cache --fix",
    "format": "prettier --write **/*.{js,md}",
    "compile": "babel src/ -d --out-dir dist/ --ignore __tests__,__mocks__",
    "compile:watch": "babel src/ --watch -d --out-dir dist/ --ignore __tests__,__mocks__",
    "prepublishOnly": "npm run compile",
    "mocha": "mocha",
    "test": "npm run unit && npm run e2e",
    "unit": "jest --config ./jest.unit.config.js ",
    "e2e": "jest --config ./jest.e2e.config.js "
  },
  "files": [
    "dist",
    "config.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-styleguidist/vue-docgen-api.git"
  },
  "keywords": [
    "vue",
    "documentation-generation",
    "jsdoc",
    "parse"
  ],
  "author": "Rafael Escala",
  "license": "MIT",
  "homepage": "https://github.com/vue-styleguidist/vue-docgen-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "doctrine": "^2.1.0",
    "hash-sum": "^1.0.2",
    "htmlparser2": "^3.9.2",
    "jsdoc-api": "^4.0.3",
    "lru-cache": "^4.1.3",
    "pug-code-gen": "^1.1.1",
    "pug-lexer": "^3.1.0",
    "pug-parser": "^2.0.2",
    "pug-runtime": "^2.0.4",
    "vue": "^2.4.2",
    "vue-template-compiler": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/cli": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "vue-property-decorator": "^7.2.0"
  },
  "peerDependencies": {
    "babel-core": "^6.0.0 || ^7.0.0-0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}
